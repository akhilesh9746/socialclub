php_value include_path ".:classes:controllers:utility"
php_value magic_quotes_gpc "Off"
php_value register_globals "Off"
php_value allow_call_time_pass_reference "On"
Options -Indexes
DirectoryIndex index.php

RewriteEngine on
RewriteRule ^members/([a-z_]+)/([a-z0-9_\-]+)/([0-9]+) index.php?page=$1&action=$2&object=$3 [QSA]
RewriteRule ^members/([a-z_]+)/([a-z0-9_\-]+) index.php?page=$1&action=$2 [QSA]
RewriteRule ^members/([a-z_]+) index.php?page=$1 [QSA]
